<div class="container service-service-c">
    <div class="row">
        <div class="col-lg-12 service-service-title text-center">
            <h6>OUR SERVICE</h6>
            <h1>What We <b>Offers.</b></h1>
        </div>
    </div>
    <%while (serviceIndex < services.length) {
        if (serviceIndex % 4 === 0) {%>
            <div class="row justify-content-around service-service-line">
                <%for (var i = 0; i < 4; i++) {
                    if (serviceIndex === services.length) {break;}%>
                    <div class="col-lg-2 service-services text-center">
                        <img src="<%=services[serviceIndex].icon%>"/>
                        <h4><b><%=services[serviceIndex].title%></b></h4>
                        <span><%=services[serviceIndex].content%></span>
                    </div>
                <%serviceIndex++;}%>
            </div>
    <%}}%>
    <div class="row"></div>
</div>